ventListener(\"click\", () => {\n          const name1 = document.getElementById(\"name1-2\").value.trim();\n          const name2 = document.getElementById(\"name2-2\").value.trim();\n          const resultText = detectLove(name1, name2);\n\n          document.getElementById(\"name1-2\").value = \"\";\n          document.getElementById(\"name2-2\").value = \"\";\n\n          const resultEl = document.getElementById(\"result\");\n          if (resultEl) resultEl.innerText = resultText;\n        });\n      });\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Detector de Amor"}]},{"tagName":"meta","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width,initial-scale=1"}},{"tagName":"meta","void":true,"attributes":{"name":"robots","content":"index,follow"}}]},"docEl":{"tagName":"html"}},"id":"a5qL07f5OV3teNtp","minHeight":"500px"}],"id":"hx3efluhOWOIkdet"}],"symbols":[],"custom":{"projectType":"web","id":""}}